+package com.example.takedelivery.model;
+
+import com.example.takedelivery.StatusPedido;
+
+public class Pedido {
+    private static int contadorId = 0;
+
+    int id;
+    String nomeCliente; //mudar para objeto cliente
+    String data;
+    String hora;
+    String endereco;
